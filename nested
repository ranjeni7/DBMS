SQL> SELECT c.customer_id, c.name, b.branch_id, b.branch_name,
  2         SUM(l.loan_amount) AS total_loan,
  3         AVG(l.loan_amount) AS average_loan,
  4         MIN(l.loan_amount) AS min_loan,
  5         MAX(l.loan_amount) AS max_loan
  6  FROM customers c
  7  JOIN loans l ON c.customer_id = l.customer_id
  8  JOIN branches b ON c.branch_id = b.branch_id
  9  WHERE l.loan_amount < 50000
 10  GROUP BY c.customer_id, c.name, b.branch_id, b.branch_name;
JOIN branches b ON c.branch_id = b.branch_id
     *
